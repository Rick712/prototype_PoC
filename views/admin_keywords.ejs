<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="/style/style.css">
    <title>Admin</title>
</head>

<body class="admin">

    <% var i = '9999'; %>
    <% var count = 0 %>
    <% for (var word in books) { %>
        <% if (count != 10) { %>
            <div class="genre" id="next<%= i %>">    
                <h2><%= books[count].Steekwoorden %></h2>
                <div class="buttons">
                    <a id="dislike" data-steekwoord="<%= books[count].Steekwoorden %>" href="#next<%= i - 1 %>">Dislike</a>
                    <a id="like" data-steekwoord="<%= books[count].Steekwoorden %>" href="#next<%= i - 1 %>">Like</a>
                </div>
            </div>
        <% count = count + 1 %> 
        <% } %>
        <% i = i - 1 %>
    <% } %>

    <div class="genre" id="next<%= i %>">
        <a id="send">Verstuur lijst</a>
        <a id="restart">Begin opnieuw</a>
    </div>

    <script src="/socket.io/socket.io.js"></script>
    <script src="script/adminKeywords.js"></script>
</body>

</html>